OPENQASM 2.0;
include "qelib1.inc";
qreg q[3];
qreg a[2];
creg c[3];
creg syn[2];
u3(pi,0,pi) q[0];
barrier q[0],q[1],q[2];
u3(pi/2,0,pi) a[0];
cz q[0],a[0];
cz q[1],a[0];
u3(pi/2,0,pi) a[0];
u3(pi/2,0,pi) a[1];
cz q[1],a[1];
cz q[2],a[1];
u3(pi/2,0,pi) a[1];
measure a[0] -> syn[0];
measure a[1] -> syn[1];
if(syn==1) u3(pi,0,pi) q[0];
measure q[0] -> c[0];
if(syn==2) u3(pi,0,pi) q[2];
if(syn==3) u3(pi,0,pi) q[1];
measure q[1] -> c[1];
measure q[2] -> c[2];
